{
  "manifest_version": 3,
  "name": "VistaTab",
  "version": "1.1.0",
  "description": "简约的新标签页",
  "icons": {
    "16": "icons/icons_16.png",
    "48": "icons/icons_48.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icons_16.png",
      "48": "icons/icons_48.png",
      "128": "icons/icon_128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://suggestion.baidu.com/*"
  ]
}
